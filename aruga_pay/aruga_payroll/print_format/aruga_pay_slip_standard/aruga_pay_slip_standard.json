 "css": "@page {\n    size: 21.6cm 27.9cm portrait;\n    margin:.5cm;\n    padding: 0;\n}\n\n.xsmall {font-size: 9px;}\n\n.table1 {border: 1px solid black;}\n\n.table2 {border: none;}\n\n.top-border {border-top: 1px solid black;}\n\n.bottom-border {border-bottom: 1px solid black;}\n\n.right-border {border-right: 1px solid black;}\n\n.no-margin-padding {\n    border-spacing: 0;\n}\n.table-heading, .contents {\n    display: flex;\n    justify-content: space-between;\n}\n\n.start {\n    justify-content: flex-start;\n}\n.center {\n    justify-content: center;\n}\n.end {\n    justify-content: flex-end;\n}\ntd, tr, table {\n    margin-top: 0;\n    padding-top: 0;\n    margin-bottom: 0;\n    padding-bottom: 0;\n    border-collapse: collapse;\n    border-spacing: 0;\n}",
 "custom_format": 1,
